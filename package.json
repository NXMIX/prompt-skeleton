{
  "name": "@nxmix/prompt-skeleton",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/NXMIX/emoji-seq-match.git"
  },
  "files": [
    "dist"
  ],
  "version": "0.0.0-development",
  "scripts": {
    "lint": "tslint -p tsconfig.json -t codeFrame 'src/**/*.ts'",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "example": "ts-node-dev examples/counter.ts",
    "dev": "ts-node-dev --respawn --type-check examples/counter.ts",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "emoji-sequences",
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 90,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true
  },
  "license": "MIT",
  "devDependencies": {
    "@nxmix/readable-ansi": "^1.0.1",
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/strip-ansi": "^3.0.0",
    "@types/through2": "^2.0.33",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.5.2",
    "strip-ansi": "^4.0.0",
    "through2": "^2.0.3",
    "ts-jest": "^22.4.6",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.2",
    "validate-commit-msg": "^2.14.0",
    "travis-deploy-once": "^5.0.0"
  },
  "dependencies": {
    "@derhuerst/cli-on-key": "^0.1.0",
    "ansi-diff-stream": "^1.2.1",
    "ansi-escapes": "^3.1.0"
  },
  "keywords": [
    "terminal",
    "command-line",
    "prompt"
  ]
}
